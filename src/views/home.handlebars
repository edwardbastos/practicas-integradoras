<div class="container">
  <h1 class="title" id="mainTitle">Makalu
    <img src="/assets/img/Makalu-logo.png" alt="">
  </h1>
</div>
<div class="container">
  {{#each listaProductos}}
  <div class="card" id={{this.id}}>
    <div class="card-body">
      <img class="card-img" src={{this.thumbnail}} alt={{this.title}} />
      <h5 class="card-title">{{this.title}}</h5>
      <div class="card-info">
        <p class="card-text">{{this.description}}</p>
        <p class="card-text">Categoría: {{this.category}}</p>
        <p class="card-text">Código: {{this.code}}</p>
        <p class="card-text">Stock: {{this.stock}}</p>
        <p class="card-text">Precio: ${{this.price}}</p>
      </div>
    </div>
  </div>
  {{/each}}
</div>